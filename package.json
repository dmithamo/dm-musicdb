  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.11.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "2.1.5"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "HOST=0.0.0.0 && PORT=8000 && ./node_modules/.bin/react-scripts start",
